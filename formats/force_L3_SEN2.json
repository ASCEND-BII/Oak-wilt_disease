{
  "description" : "Image collection format for Sentinel-2 imagery LEVEL 3 derived from FORCE",
  "tags" : ["FORCE", "LEVEL 3", "Sentinel-2", "BOA"],
  "references" : "https://force-eo.readthedocs.io/en/latest/",
  "pattern" : ".+\\.tif",
  "images" : {
    "pattern" : ".*([0-9]{4}-[0-9]{4}_[0-9]{3}-[0-9]{3}_HL_TSA_SEN2L_KNV_TSS_[0-9]{8}_SEN2[AB]).+\\.tif"
  },
  "datetime" : {
    "pattern" : ".*[0-9]{4}-[0-9]{4}_[0-9]{3}-[0-9]{3}_HL_TSA_SEN2L_KNV_TSS_([0-9]{8})_SEN2[AB].+\\.tif",
    "format" : "%Y%m%d"
  },
  "bands" : {
    "kNDVI" :{
      "band" : 1,
      "nodata" : -9999
    }
  }
}